//#include "libft.h"
#include <string.h>
#include <unistd.h>
#include <stdio.h>

int ft_isalpha(char c)
{
	if ((c >= 'a' || c <= 'z') && (c >= 'A' || c <= 'Z'))
		return (1);
	
	
}

int main()
{
	printf("%d, %d", isalpha ('F', ft_isalpha('f'));
	
}
